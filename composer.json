{
    "name": "yiiliveext/yii-debug-panels",
    "type": "lib",
    "description": "Yii Debug Panels",
    "keywords": [
        "yii3",
        "app",
        "debug",
        "panels"
    ],
    "homepage": "https://github.com/yiilivext/yii-debug-panels/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisliveext/yii-debug-panels/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii-debug-panels"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "tuupola/cors-middleware": "^1.2.1",
        "yiisoft/data-response": "^3.0@dev",
        "yiisoft/http": "^1.0@dev"
    },
    "require-dev": {        
    },
    "autoload": {
        "psr-4": {
            "Yiiliveext\\Yii\\Debug\\Panels\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin-options": {
            "output-directory": "config/packages"
        },
        "config-plugin": {
            "web": "config/web.php",
            "params": "config/params.php",
            "routes": "config/routes.php"
        }
    },
    "config": {
        "sort-packages": true
    }
}
